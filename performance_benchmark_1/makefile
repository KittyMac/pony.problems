debug:
	/Volumes/Development/Development/pony/ponyc/build/debug/ponyc -d -o ./build/
	lldb ./build/performance_benchmark_1 --ponynoblock --ponynoscale

test1:
	@echo "\n\n"
	@echo "Test 1: My fork of pony with all actor constructors set to be synchronous\n"
	/Volumes/Development/Development/pony/ponyc/build/release/ponyc -V=0 --sync-actor-constructors -o ./build/
	-./build/performance_benchmark_1 --ponynoblock --ponynoscale

test2:
	@echo "\n\n"
	@echo "Test 2: My fork of pony with all actor constructors asynchronous (like stock pony)\n"
	/Volumes/Development/Development/pony/ponyc/build/release/ponyc -V=0 -o ./build/
	-./build/performance_benchmark_1 -- --ponynoblock --ponynoscale

stock1:
	@echo "\n\n"
	@echo "Test 3: Stock pony w/ --ponynoblock (currently crashes)\n"
	ponyc -V=0 -o ./build/
	-./build/performance_benchmark_1 --ponynoblock --ponynoscale

stock2:
	@echo "\n\n"
	@echo "Test 4: Stock pony w/out --ponynoblock (slower but doesn't crash)\n"
	ponyc -V=0 -o ./build/
	-./build/performance_benchmark_1 --ponynoscale